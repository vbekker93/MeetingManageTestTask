@model IEnumerable<MeetingService.Models.Meet>

@{
    ViewBag.Title = "Index";
}

<h2>Meets table</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactPersonFullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactPersonPhone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MeetingStartDateTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MeetingEndDateTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContactPersonFullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContactPersonPhone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MeetingStartDateTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MeetingEndDateTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) 
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
