@model IEnumerable<MeetingService.Models.MeetParticipantGroup>

@{
    ViewBag.Title = "MeetParticipantGroup";
}

    <h2>Meeting Participants Report</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MeetTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ParticipantList)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MeetTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ParticipantList)
        </td>
    </tr>
}

</table>
